{
  "chainer": {
    "in": [
      "question_raw"
    ],
    "out": [
      "rank_text_score_id_ensemble"
    ],
    "pipe": [
      {
        "name": "api_router",
        "in": "question_raw",
        "n_workers": 3,
        "out": [
          "rank_text_score_id_tfidf",
          "rank_text_score_id_tfhub",
          "rank_text_score_id_rnet"
        ],
        "api_requesters": [
          {
            "name": "api_requester",
            "_comment": "Doesn't requre tf, run from odqa_pmef_tf_1.7 branch",
            "url": "http://0.0.0.0:5000/model",
            "in": "question_raw",
            "param_names": [
              "context"
            ],
            "out": [
              "rank_text_score_id_tfidf"
            ]
          },
          {
            "name": "api_requester",
            "_comment": "Requires tf 1.7 GPU, run from odqa_pmef_tf_1.7 branch",
            "url": "http://0.0.0.0:5001/model",
            "in": "question_raw",
            "param_names": [
              "context"
            ],
            "out": [
              "rank_text_score_id_tfhub"
            ]
          },
          {
            "name": "api_requester",
            "_comment": "Requires tf 1.4 GPU, run from odqa_pmef branch",
            "url": "http://0.0.0.0:5002/model",
            "in": "question_raw",
            "param_names": [
              "context"
            ],
            "out": [
              "rank_text_score_id_rnet"
            ]
          }
        ]
      },
      {
        "name": "ensemble_ranker",
        "top_n": 20,
        "in": [
          "rank_text_score_id_tfidf",
          "rank_text_score_id_tfhub",
          "rank_text_score_id_rnet"
        ],
        "out": [
          "rank_text_score_id_ensemble"
        ]
      }
    ]
  },
  "train": {
    "validate_best": false,
    "test_best": false,
    "batch_size": 10000
  },
  "metadata": {
    "download": [
      "http://lnsigo.mipt.ru/export/deeppavlov_data/pmef.tar.gz",
      {
        "url": "http://lnsigo.mipt.ru/export/datasets/pmef/en_drones.db",
        "subdir": "pmef"
      }
    ]
  }
}